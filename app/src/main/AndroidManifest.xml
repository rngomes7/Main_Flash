<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="theflash.flash" >
    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="15" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:debuggable="true">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".study"></activity>
        <activity android:name=".settings"></activity>
        <activity android:name=".save"></activity>
        <activity android:name=".edit"></activity>
        <activity android:name=".myImport"></activity>
        <activity android:name=".make"></activity>
        <activity android:name=".scores"></activity>
        <activity android:name=".study_answer"></activity>

    </application>

</manifest>
